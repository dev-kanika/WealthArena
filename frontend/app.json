{
  "expo": {
    "name": "WealthArena Investment Platform",
    "slug": "wealtharena-investment-platform-kqbxtri",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "sdkVersion": "54.0.0",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.wealtharena-investment-platform-kqbxtri",
      "statusBar": {
        "barStyle": "dark-content",
        "backgroundColor": "#000000"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.rork.wealtharena-investment-platform-kqbxtri",
      "navigationBar": {
        "visible": true,
        "backgroundColor": "#000000",
        "barStyle": "light-content"
      },
      "statusBar": {
        "barStyle": "dark-content",
        "backgroundColor": "#000000"
      },
      "softwareKeyboardLayoutMode": "resize"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-system-ui",
        {
          "androidNavigationBarBackgroundColor": "#000000",
          "androidNavigationBarStyle": "light-content"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": false
    }
  }
}
